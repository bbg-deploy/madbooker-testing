
.pricing_holder
  - if room.discounted_rate.nil?
    .row
      .label
        Room Rate:
      .price= format room.rate
  - else
    .row
      .label
        Original Room Rate:
      .old_pice= format room.rate
    .row
      .label
        Discounted Room Rate:
      .price= format room.discounted_rate
