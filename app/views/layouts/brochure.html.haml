!!!
%html
  %head
    %title= title
    = stylesheet_link_tag 'brochure', media: "all", "data-turbolinks-track" => true 
    = javascript_include_tag "application", "data-turbolinks-track" => true 
    = csrf_meta_tag
    = yield(:head)
    = render "layouts/setup"


    = render "layouts/ga/global"
    = render "layouts/ga/brochure"
  %body#brochure
    #header
      .container    
        = link_to image_tag('madbooker_logo_smaller.png'), root_path, :class=>"logo"
        #nav
          = render :partial => '/layouts/brochure_nav'
    #content
      //%h1= yield(:title)
      - unless notice.blank?
        %p.notice= notice
      - unless alert.blank?
        %p.alert= alert
      = yield
    = render :partial => '/layouts/footer'
    = render "layouts/end_of_body"
