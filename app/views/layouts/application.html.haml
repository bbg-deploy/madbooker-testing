!!!
%html
  %head
    %title= title
    = stylesheet_link_tag 'application', media: "all", "data-turbolinks-track" => true 
    = javascript_include_tag "application", "data-turbolinks-track" => true 
    = csrf_meta_tag
    = yield(:head)
  %body
    #container
      #header
        #nav
          = render :partial => '/layouts/nav'
      #content
        %h1= yield(:title)
        - unless notice.blank?
          %p.notice= notice
        - unless alert.blank?
          %p.alert= alert
        = yield
    = render :partial => '/layouts/footer'
